{
    "version": 1,
    "projects": {
        "shell": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/shell",
            "sourceRoot": "apps/shell/src",
            "prefix": "mfe-nx-demo",
            "architect": {
                "build": {
                    "builder": "ngx-build-plus:browser",
                    "options": {
                        "outputPath": "dist/apps/shell",
                        "index": "apps/shell/src/index.html",
                        "main": "apps/shell/src/main.ts",
                        "polyfills": "apps/shell/src/polyfills.ts",
                        "tsConfig": "apps/shell/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/shell/src/favicon.ico",
                            "apps/shell/src/assets"
                        ],
                        "styles": [
                            "apps/shell/src/styles.scss"
                        ],
                        "scripts": [],
                        "extraWebpackConfig": "apps/shell/webpack.config.js"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/shell/src/environments/environment.ts",
                                    "with": "apps/shell/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "extraWebpackConfig": "apps/shell/webpack.prod.config.js"
                        }
                    }
                },
                "serve": {
                    "builder": "ngx-build-plus:dev-server",
                    "options": {
                        "browserTarget": "shell:build",
                        "extraWebpackConfig": "apps/shell/webpack.config.js",
                        "port": 4200
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "shell:build:production",
                            "extraWebpackConfig": "apps/shell/webpack.prod.config.js"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "ngx-build-plus:extract-i18n",
                    "options": {
                        "browserTarget": "shell:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/shell/src/**/*.ts"
                        ]
                    }
                },
                "test": {
                    "builder": "ngx-build-plus:karma",
                    "options": {
                        "jestConfig": "apps/shell/jest.config.js",
                        "passWithNoTests": true,
                        "extraWebpackConfig": "apps/shell/webpack.config.js"
                    }
                }
            }
        },
        "auth-store": {
            "projectType": "library",
            "root": "libs/auth-store",
            "sourceRoot": "libs/auth-store/src",
            "prefix": "mfe-nx-demo",
            "architect": {
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "libs/auth-store/src/**/*.ts"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/auth-store/jest.config.js",
                        "passWithNoTests": true
                    }
                }
            },
            "schematics": {}
        },
        "counter": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/counter",
            "sourceRoot": "apps/counter/src",
            "prefix": "mfe-nx-demo",
            "architect": {
                "build": {
                    "builder": "ngx-build-plus:browser",
                    "options": {
                        "outputPath": "dist/apps/counter",
                        "index": "apps/counter/src/index.html",
                        "main": "apps/counter/src/main.ts",
                        "polyfills": "apps/counter/src/polyfills.ts",
                        "tsConfig": "apps/counter/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/counter/src/favicon.ico",
                            "apps/counter/src/assets"
                        ],
                        "styles": [
                            "apps/counter/src/styles.scss"
                        ],
                        "scripts": [],
                        "extraWebpackConfig": "apps/counter/webpack.config.js"
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/counter/src/environments/environment.ts",
                                    "with": "apps/counter/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "extraWebpackConfig": "apps/counter/webpack.prod.config.js"
                        }
                    }
                },
                "serve": {
                    "builder": "ngx-build-plus:dev-server",
                    "options": {
                        "browserTarget": "counter:build",
                        "extraWebpackConfig": "apps/counter/webpack.config.js",
                        "port": 3000
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "counter:build:production",
                            "extraWebpackConfig": "apps/counter/webpack.prod.config.js"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "counter:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/counter/src/**/*.ts"
                        ]
                    }
                },
                "test": {
                    "builder": "ngx-build-plus:karma",
                    "options": {
                        "jestConfig": "apps/counter/jest.config.js",
                        "passWithNoTests": true,
                        "extraWebpackConfig": "apps/counter/webpack.config.js"
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "packageManager": "yarn"
    },
    "schematics": {
        "@nrwl/angular": {
            "application": {
                "linter": "eslint"
            },
            "library": {
                "linter": "eslint"
            },
            "storybook-configuration": {
                "linter": "eslint"
            }
        },
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    },
    "resolutions": {
        "webpack": "^5.0.0"
    },
    "defaultProject": "shell"
}
